{
  "name": "regexpstructor",
  "version": "1.1.3",
  "description": "Build regular expressions in a safe and readable way.",
  "keywords": [
    "VerbalExpressions",
    "Regular",
    "Expressions",
    "RegExp",
    "RegEx",
    "fluid"
  ],
  "author": "Jonas Winzen <2ndjpeg+npmjs@gmail.com>",
  "homepage": "https://github.com/mister-what/regexpstructor#readme",
  "license": "MIT",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "exports": {
    "import": "./dist/index.mjs",
    "require": "./dist/index.cjs"
  },
  "sideEffects": false,
  "types": "types/index.d.ts",
  "directories": {
    "src": "src"
  },
  "files": [
    "dist",
    "types"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mister-what/regexpstructor.git"
  },
  "scripts": {
    "clean": "rimraf dist",
    "clean:reason": "bsb -clean-world",
    "build:reason": "bsb -make-world",
    "build:esbuild": "node esbuild.cjs",
    "build": "run-s build:*",
    "prepack": "npm-run-all -s clean build",
    "pretest": "npm run lint",
    "test": "jest --coverage",
    "lint": "eslint . --cache --fix"
  },
  "bugs": {
    "url": "https://github.com/mister-what/regexpstructor/issues"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/cli": "^7.13.10",
    "@babel/core": "^7.13.10",
    "@babel/eslint-parser": "^7.13.10",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-proposal-private-methods": "^7.13.0",
    "@babel/preset-env": "^7.13.10",
    "@rescript/std": "^9.0.1",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/commit-analyzer": "^8.0.1",
    "@semantic-release/git": "^9.0.0",
    "@semantic-release/github": "^7.2.0",
    "@semantic-release/npm": "^7.0.10",
    "@semantic-release/release-notes-generator": "^9.0.2",
    "babel-plugin-macros": "^3.0.1",
    "bs-platform": "^9.0.2",
    "commitizen": "^4.2.3",
    "cz-conventional-changelog": "^3.3.0",
    "esbuild": "^0.9.2",
    "esbuild-plugin-babel": "^0.2.3",
    "eslint": "^7.22.0",
    "eslint-plugin-import": "^2.22.1",
    "fast-fp.macro": "^1.0.0",
    "husky": "=4",
    "jest": "^26.6.3",
    "lint-staged": ">=10",
    "npm-run-all": "^4.1.5",
    "param.macro": "3.2.1",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "semantic-release": "^17.4.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "./{src,test}/**/*.js": "eslint --cache --fix",
    "./{src,test,types}/**/*.{js,ts,md}": "prettier --write"
  }
}
